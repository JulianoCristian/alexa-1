{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "globals": {
    "Promise": true,
    "fetch": true
  },
  "rules": {
    "indent": ["error", 2],
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "no-undef": [2, {  }],
    "no-unused-vars": [2, { "varsIgnorePattern": "^ssml$" }]
  }
}

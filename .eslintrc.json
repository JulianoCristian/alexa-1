{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "globals": {
    "Promise": true,
    "fetch": true
  },
  "rules": {
    "no-undef": ["error"],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^ssml$", "args": "after-used" }]
  }
}
